<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean id="employeeBeanObject" class="com.springCore.classs1.ObjectInjection.Employee" scope="prototype" lazy-init="true" >
<property name="id" value="1001"/>
<property name="name" value="Amit"/> 
<property name="address" ref="addressObject"  />
</bean>
<bean id="employeeBeanObject2" class="com.springCore.classs1.ObjectInjection.Employee">
<property name="address" ref="addressObject"  />    
</bean>
<bean id="addressObject" class="com.springCore.classs1.ObjectInjection.Address">
     <property name="city" value="Delhi"/>
    <property name="state" value="Delhi"/>
    <property name="zipCode" value="110007"/> 
</bean>
</beans>